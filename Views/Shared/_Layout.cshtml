<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AspNetCore2VueJsTemplate</title>
    <base href="~/" />

    <link rel="stylesheet" href="~/dist/app.css" asp-append-version="true" />
</head>
<body>
    @RenderBody()

    <script src="~/dist/app.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
